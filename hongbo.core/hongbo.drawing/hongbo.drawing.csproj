<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>hongbo.json 的  Json 工具包，对 System.Drawing 进行了一些扩展;</Description>
    <AssemblyVersion>0.19.0327.1807</AssemblyVersion>
    <FileVersion>0.19.0327.1807</FileVersion>
    <Version>0.19.0327</Version>
    <PackageLicenseExpression></PackageLicenseExpression>

<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>hongbo.Sms</AssemblyName>
    <RootNamespace>hongbo.Sms</RootNamespace>
    <Description>高德地图的Webservice的c#</Description>
    <Copyright>http://www.gnu.org/licenses/licenses.html</Copyright>
    <PackageProjectUrl>https://github.com/hesi726/hongbo</PackageProjectUrl>
